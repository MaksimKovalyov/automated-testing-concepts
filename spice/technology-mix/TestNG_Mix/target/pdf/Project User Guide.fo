<fo:block wrap-option="no-wrap" font-size="10pt" keep-together="always" font-family="monospace" border-width="0.5pt" color="black" padding-before="0.25em" border-style="solid" start-indent="inherited-property-value(start-indent) + 2.5em" space-after="1em" end-indent="inherited-property-value(end-indent) + 3em" linefeed-treatment="preserve" white-space-collapse="false" padding-start="0.25em" padding-end="0.25em" space-before="0.75em" padding-after="0.25em" border-color="#454545">^L
</fo:block>

